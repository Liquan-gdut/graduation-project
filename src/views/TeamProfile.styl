.team-profile
  color rgba(0,0,0, .7)

.team
  width 100%
  padding 50px 0 100px
  h3
    padding 0
  .title
    font-family:SourceHanSansCN-Medium;
    margin-bottom 20px
  .introduction
    display inline-block
    width 740px
    font-family SourceHanSansCN-Normal
    color rgba(102,102,102,1)
    line-height 30px

.box
  width: 100%;
  height auto
  background:rgba(250,250,250,1);
  text-align left
  padding: 50px 0
  .title
    font-family:SourceHanSansCN-Medium;
    padding-top: 0;

.avator
  width: 382px;
  height: 572px;
  position absolute
  right 0
  bottom: 0;

.msg
  width: 572px;
  line-height: 24px
  font-family:SourceHanSansCN-Normal;

a:link {color:#4285F4;}
a:visited {color:#4285F4;}
a:hover {color:red;}
a:active {color:yellow;}

.list-item
  font-family:SourceHanSansCN-Normal;
  line-height:24px;
  .icon
    display: inline-block;
    margin-right: 10px;
    margin-left 2px
    width: 5px;
    height: 5px;
    background: #333333;
    vertical-align: middle;
    top: -2px;
    position: relative;
    font-family:SourceHanSansCN-Normal;
    font-weight:400;

.subtitle
  list-style none
  position relative
  font-family:SourceHanSansCN-Medium;
  color:rgba(51,51,51,1);
  font-size 24px
  font-weight:500;
  margin 50px auto 20px

.wraper
  width 460px
  overflow hidden
  ul
    margin-top 50px
.paragraph
  margin-top 20px